{
    "installCommand" : "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        },
        {
            "src": "./**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.ts"
        }
    ]

}